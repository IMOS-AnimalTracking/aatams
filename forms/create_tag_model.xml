<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="xsltforms/xsltforms.xsl" type="text/xsl"?><xhtml:html xmlns:ows="http://www.opengis.net/ows" xmlns:gml="http://www.opengis.net/gml"
            xmlns:xhtml="http://www.w3.org/1999/xhtml"
            xmlns:aatams="http://www.imos.org.au/aatams"
            xmlns:xf="http://www.w3.org/2002/xforms"
            xmlns:ev="http://www.w3.org/2001/xml-events"
            xmlns:wfs="http://www.opengis.net/wfs"
            xmlns:ogc="http://www.opengis.net/ogc">
   <xhtml:head>
      <xhtml:title>AATAMS Web Interface</xhtml:title>
      <xhtml:link href="aatams.css" rel="stylesheet" type="text/css"/>
      <xhtml:script type="text/javascript" src="aatams_xforms.js" charset="UTF-8"/>
      <xf:model id="model1">
         <xf:instance id="inst_data">
            <wfs:Transaction version="1.1.0" service="WFS">
               <wfs:Insert>
                  <wfs:FeatureCollection>
                     <gml:featureMember>
                        <aatams:tag_model>
                           <aatams:tag_family_ref>
                              <aatams:tag_family/>
                           </aatams:tag_family_ref>
                           <aatams:vue_coding/>
                           <aatams:tag_type_ref>
                              <aatams:tag_type/>
                           </aatams:tag_type_ref>
                           <aatams:frequency/>
                           <aatams:sync/>
                           <aatams:bin/>
                           <aatams:min/>
                           <aatams:max/>
                        </aatams:tag_model>
                     </gml:featureMember>
                  </wfs:FeatureCollection>
               </wfs:Insert>
            </wfs:Transaction>
         </xf:instance>
         <xf:instance id="inst_subfeatures">
            <data>
               <device_model_id>aatams.device_model.1</device_model_id>
               <tag_family_id>aatams.tag_family.1</tag_family_id>
               <tag_type_id>aatams.tag_type.1</tag_type_id>
            </data>
         </xf:instance>
         <xf:instance id="inst_device_model"
                      src="../../deegree-wfs/services?service=WFS&amp;version=1.1.0&amp;request=GetFeature&amp;namespace=xmlns(aatams=http://www.imos.org.au/aatams)&amp;typename=aatams:device_model"/>
         <xf:instance id="inst_tag_family"
                      src="../../deegree-wfs/services?service=WFS&amp;version=1.1.0&amp;request=GetFeature&amp;namespace=xmlns(aatams=http://www.imos.org.au/aatams)&amp;typename=aatams:tag_family"/>
         <xf:instance id="inst_tag_type"
                      src="../../deegree-wfs/services?service=WFS&amp;version=1.1.0&amp;request=GetFeature&amp;namespace=xmlns(aatams=http://www.imos.org.au/aatams)&amp;typename=aatams:tag_type"/>
         <xf:instance id="inst_response">
            <dummy>
               <ows:Exception/>
               <wfs:TransactionResponse>
                  <wfs:InsertResults>
                     <wfs:Feature>
                        <ogc:FeatureId/>
                     </wfs:Feature>
                  </wfs:InsertResults>
               </wfs:TransactionResponse>
            </dummy>
         </xf:instance>
         <xf:bind id="tag_family" nodeset="instance('inst_subfeatures')//tag_family_id"
                  type="xsd:string"
                  required="true()"/>
         <xf:bind id="vue_coding" nodeset="instance('inst_data')//aatams:vue_coding"
                  type="xsd:string"
                  required="true()"/>
         <xf:bind id="tag_type" nodeset="instance('inst_subfeatures')//tag_type_id"
                  type="xsd:string"
                  required="true()"/>
         <xf:bind id="frequency" nodeset="instance('inst_data')//aatams:frequency"
                  type="xsd:string"
                  required="true()"/>
         <xf:bind id="sync" nodeset="instance('inst_data')//aatams:sync" type="xsd:string"
                  required="true()"/>
         <xf:bind id="bin" nodeset="instance('inst_data')//aatams:bin" type="xsd:string"
                  required="true()"/>
         <xf:bind id="min" nodeset="instance('inst_data')//aatams:min" type="xsd:integer"
                  required="true()"/>
         <xf:bind id="max" nodeset="instance('inst_data')//aatams:max" type="xsd:integer"
                  required="true()"/>
         <xf:bind id="error_message" nodeset="instance('inst_response')//ServiceException"
                  type="xsd:string"/>
         <xf:bind id="success_message" nodeset="instance('inst_response')//ogc:FeatureId/@fid"
                  type="xsd:string"/>
         <xf:submission id="s01" ref="instance('inst_data')" method="post"
                        action="../../deegree-wfs/services"
                        replace="instance"
                        instance="inst_response">
            <xf:action ev:event="xforms-submit">
               <xf:insert nodeset="instance('inst_data')//aatams:device_model_ref/aatams:device_model"
                          origin="instance('inst_device_model')//aatams:device_model[@gml:id=instance('inst_subfeatures')/device_model_id]"/>
               <xf:delete nodeset="instance('inst_data')//aatams:device_model_ref/aatams:device_model"
                          at="1"/>
               <xf:insert nodeset="instance('inst_data')//aatams:tag_family_ref/aatams:tag_family"
                          origin="instance('inst_tag_family')//aatams:tag_family[@gml:id=instance('inst_subfeatures')/tag_family_id]"/>
               <xf:delete nodeset="instance('inst_data')//aatams:tag_family_ref/aatams:tag_family" at="1"/>
               <xf:insert nodeset="instance('inst_data')//aatams:tag_type_ref/aatams:tag_type"
                          origin="instance('inst_tag_type')//aatams:tag_type[@gml:id=instance('inst_subfeatures')/tag_type_id]"/>
               <xf:delete nodeset="instance('inst_data')//aatams:tag_type_ref/aatams:tag_type" at="1"/>
            </xf:action>
            <xf:message level="modeless" ev:event="xforms-submit-error">
									Submit error.
								</xf:message>
         </xf:submission>
      </xf:model>
   </xhtml:head>
   <xhtml:body><!--Debug--><xhtml:div class="form">
         <legend>ADD TAG MODEL</legend>
         <xf:select1 bind="tag_family" appearance="minimal" incremental="true()">
            <xf:label>Tag Family</xf:label>
            <xf:itemset nodeset="instance('inst_tag_family')//aatams:tag_family">
               <xf:value ref="@gml:id"/>
               <xf:label ref="aatams:name"/>
            </xf:itemset>
         </xf:select1>
         <xf:input bind="vue_coding" incremental="true()">
            <xf:label>Vue Coding</xf:label>
         </xf:input>
         <xf:select1 bind="tag_type" appearance="minimal" incremental="true()">
            <xf:label>Tag Type</xf:label>
            <xf:itemset nodeset="instance('inst_tag_type')//aatams:tag_type">
               <xf:value ref="@gml:id"/>
               <xf:label ref="aatams:name"/>
            </xf:itemset>
         </xf:select1>
         <xf:input bind="frequency" incremental="true()">
            <xf:label>Frequency</xf:label>
         </xf:input>
         <xf:input bind="sync" incremental="true()">
            <xf:label>Sync</xf:label>
         </xf:input>
         <xf:input bind="bin" incremental="true()">
            <xf:label>Bin</xf:label>
         </xf:input>
         <xf:input bind="min" incremental="true()">
            <xf:label>Min</xf:label>
         </xf:input>
         <xf:input bind="max" incremental="true()">
            <xf:label>Max</xf:label>
         </xf:input>
         <xf:submit submission="s01">
            <xf:label>Save</xf:label>
         </xf:submit>
         <xf:trigger>
            <xf:label>Reset</xf:label>
            <xf:reset ev:event="DOMActivate"/>
         </xf:trigger>
         <xf:group>
            <xf:output bind="error_message" class="error">
               <xf:label>Error:</xf:label>
            </xf:output>
            <xf:output bind="success_message">
               <xf:label>New Record Id:</xf:label>
            </xf:output>
         </xf:group>
      </xhtml:div>
      <xhtml:br/>
      <xhtml:div id="console"/>
   </xhtml:body>
</xhtml:html>