<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="xsltforms-beta/xsltforms/xsltforms.xsl" type="text/xsl"?><html xmlns:aatams="http://www.imos.org.au/aatams" xmlns="http://www.w3.org/1999/xhtml"
      xmlns:xf="http://www.w3.org/2002/xforms"
      xmlns:ev="http://www.w3.org/2001/xml-events">
   <head>
      <title>AATAMS Web Interface</title>
      <link href="aatams.css" rel="stylesheet" type="text/css"/>
      <xf:model>
         <xf:instance id="inst_request">
            <wfs:GetFeature xmlns:wfs="http://www.opengis.net/wfs" xmlns:gml="http://www.opengis.net/gml"
                            xmlns:ogc="http://www.opengis.net/ogc"
                            outputFormat="text/xml; subtype=gml/3.1.1">
               <wfs:Query typeName="pre3:tag_model">
                  <ogc:Filter>
                     <ogc:GmlObjectId gml:id=""/>
                  </ogc:Filter>
               </wfs:Query>
               <aatams:dummy/>
            </wfs:GetFeature>
         </xf:instance>
         <xf:instance id="inst_feature_id">
            <id xmlns="">aatams.tag_model.NNNN</id>
         </xf:instance>
         <xf:bind id="feature_id" nodeset="instance('inst_feature_id')" type="xsd:string"/>
         <xf:instance id="inst_response">
            <wfs:FeatureCollection xmlns:wfs="http://www.opengis.net/wfs" xmlns:gml="http://www.opengis.net/gml">
               <gml:featureMember>
                  <aatams:tag_model/>
               </gml:featureMember>
            </wfs:FeatureCollection>
         </xf:instance>
         <xf:bind id="tag_family"
                  nodeset="instance('inst_response')//aatams:tag_family/aatams:name"
                  type="xsd:string"/>
         <xf:bind id="vue_coding"
                  nodeset="instance('inst_response')//aatams:tag_model/aatams:vue_coding"
                  type="xsd:string"/>
         <xf:bind id="tag_type" nodeset="instance('inst_response')//aatams:tag_type/aatams:name"
                  type="xsd:string"/>
         <xf:bind id="frequency"
                  nodeset="instance('inst_response')//aatams:tag_model/aatams:frequency"
                  type="xsd:string"/>
         <xf:bind id="sync" nodeset="instance('inst_response')//aatams:tag_model/aatams:sync"
                  type="xsd:string"/>
         <xf:bind id="bin" nodeset="instance('inst_response')//aatams:tag_model/aatams:bin"
                  type="xsd:string"/>
         <xf:bind id="min" nodeset="instance('inst_response')//aatams:tag_model/aatams:min"
                  type="xsd:string"/>
         <xf:bind id="max" nodeset="instance('inst_response')//aatams:tag_model/aatams:max"
                  type="xsd:string"/>
         <xf:submission id="s01" method="post" action="../deegree-wfs/services"
                        ref="instance('inst_request')"
                        replace="instance"
                        instance="inst_response">
            <xf:message level="modeless" ev:event="xforms-submit-error">Submit error.</xf:message>
         </xf:submission>
      </xf:model>
   </head>
   <body>
      <fieldset xmlns="">
         <legend>VIEW TAG MODEL</legend>
         <table xmlns="http://www.w3.org/1999/xhtml">
            <tbody>
               <tr>
                  <td>
                     <xf:input bind="feature_id">
                        <xf:label>Enter Tag Model Id</xf:label>
                     </xf:input>
                  </td>
                  <td>
                     <xf:trigger>
                        <xf:label>Search</xf:label>
                        <xf:action ev:event="DOMActivate">
                           <xf:setvalue ref="instance('inst_request')//@gml:id" value="instance('inst_feature_id')"/>
                           <xf:send submission="s01"/>
                        </xf:action>
                     </xf:trigger>
                  </td>
               </tr>
            </tbody>
         </table>
         <xf:output bind="tag_family">
            <xf:label>Tag Family:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="vue_coding">
            <xf:label>Vue Coding:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="tag_type">
            <xf:label>Tag Type:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="frequency">
            <xf:label>Frequency:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="sync">
            <xf:label>Sync:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="bin">
            <xf:label>Bin:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="min">
            <xf:label>Min:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="max">
            <xf:label>Max:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
      </fieldset>
      <br/>
      <div id="console" style="display:none;"/>
   </body>
</html>