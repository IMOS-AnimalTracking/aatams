<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="xsltforms/xsltforms.xsl" type="text/xsl"?><html xmlns:aatams="http://www.imos.org.au/aatams"
      xmlns:xf="http://www.w3.org/2002/xforms"
      xmlns:ev="http://www.w3.org/2001/xml-events"
      xmlns:wfs="http://www.opengis.net/wfs"
      xmlns:ogc="http://www.opengis.net/ogc"
      xmlns:ows="http://www.opengis.net/ows"
      xmlns="http://www.w3.org/1999/xhtml"
      xmlns:gml="http://www.opengis.net/gml">
   <head>
      <title>AATAMS Web Interface</title>
      <link href="aatams.css" rel="stylesheet" type="text/css"/>
      <xf:model id="model1">
         <xf:instance id="inst_data">
            <wfs:Transaction version="1.1.0" service="WFS">
               <wfs:Insert>
                  <wfs:FeatureCollection>
                     <gml:featureMember>
                        <aatams:organisation>
                           <aatams:name/>
                           <aatams:phone_number/>
                           <aatams:fax_number/>
                           <aatams:physical_address_1/>
                           <aatams:physical_address_2/>
                           <aatams:physical_address_state/>
                           <aatams:physical_address_country/>
                           <aatams:physical_address_postcode/>
                           <aatams:postal_address_1/>
                           <aatams:postal_address_2/>
                           <aatams:postal_address_state/>
                           <aatams:postal_address_country/>
                           <aatams:postal_address_postcode/>
                        </aatams:organisation>
                     </gml:featureMember>
                  </wfs:FeatureCollection>
               </wfs:Insert>
            </wfs:Transaction>
         </xf:instance>
         <xf:instance id="inst_subfeatures">
            <data xmlns=""/>
         </xf:instance>
         <xf:instance id="inst_response">
            <dummy xmlns="">
               <ows:Exception/>
               <wfs:TransactionResponse>
                  <wfs:InsertResults>
                     <wfs:Feature>
                        <ogc:FeatureId/>
                     </wfs:Feature>
                  </wfs:InsertResults>
               </wfs:TransactionResponse>
            </dummy>
         </xf:instance>
         <xf:bind id="name" nodeset="instance('inst_data')//aatams:name" type="xsd:string"
                  required="true()"/>
         <xf:bind id="phone_number" nodeset="instance('inst_data')//aatams:phone_number"
                  type="xsd:string"
                  required="true()"/>
         <xf:bind id="fax_number" nodeset="instance('inst_data')//aatams:fax_number"
                  type="xsd:string"
                  required="false()"/>
         <xf:bind id="physical_address_1"
                  nodeset="instance('inst_data')//aatams:physical_address_1"
                  type="xsd:string"
                  required="false()"/>
         <xf:bind id="physical_address_2"
                  nodeset="instance('inst_data')//aatams:physical_address_2"
                  type="xsd:string"
                  required="false()"/>
         <xf:bind id="physical_address_state"
                  nodeset="instance('inst_data')//aatams:physical_address_state"
                  type="xsd:string"
                  required="false()"/>
         <xf:bind id="physical_address_country"
                  nodeset="instance('inst_data')//aatams:physical_address_country"
                  type="xsd:string"
                  required="false()"/>
         <xf:bind id="physical_address_postcode"
                  nodeset="instance('inst_data')//aatams:physical_address_postcode"
                  type="xsd:string"
                  required="false()"/>
         <xf:bind id="postal_address_1" nodeset="instance('inst_data')//aatams:postal_address_1"
                  type="xsd:string"
                  required="false()"/>
         <xf:bind id="postal_address_2" nodeset="instance('inst_data')//aatams:postal_address_2"
                  type="xsd:string"
                  required="false()"/>
         <xf:bind id="postal_address_state"
                  nodeset="instance('inst_data')//aatams:postal_address_state"
                  type="xsd:string"
                  required="false()"/>
         <xf:bind id="postal_address_country"
                  nodeset="instance('inst_data')//aatams:postal_address_country"
                  type="xsd:string"
                  required="false()"/>
         <xf:bind id="postal_address_postcode"
                  nodeset="instance('inst_data')//aatams:postal_address_postcode"
                  type="xsd:string"
                  required="false()"/>
         <xf:bind id="error_message" nodeset="instance('inst_response')//ServiceException"
                  type="xsd:string"/>
         <xf:bind id="success_message" nodeset="instance('inst_response')//ogc:FeatureId/@fid"
                  type="xsd:string"/>
         <xf:submission id="s01" ref="instance('inst_data')" method="post"
                        action="../../deegree-wfs/services"
                        replace="instance"
                        instance="inst_response">
            <xf:action ev:event="xforms-submit"/>
            <xf:message level="modeless" ev:event="xforms-submit-error">
									Submit error.
								</xf:message>
         </xf:submission>
         <xf:dispatch ev:event="xforms-ready" name="set-selected" target="model1"/>
         <xf:action ev:event="set-selected">
            <xf:dispatch name="xforms-revalidate" target="model1"/>
         </xf:action>
      </xf:model>
   </head>
   <body><!--Debug--><div class="form">
         <legend>ADD ORGANISATION</legend>
         <div class="form-contents">
            <xf:input bind="name" incremental="true()">
               <xf:label>Name</xf:label>
            </xf:input>
            <xf:input bind="phone_number" incremental="true()">
               <xf:label>Phone Number</xf:label>
            </xf:input>
            <xf:input bind="fax_number" incremental="true()">
               <xf:label>Fax Number</xf:label>
            </xf:input>
            <xf:input bind="physical_address_1" incremental="true()">
               <xf:label>Physical Address 1</xf:label>
            </xf:input>
            <xf:input bind="physical_address_2" incremental="true()">
               <xf:label>Physical Address 2</xf:label>
            </xf:input>
            <xf:input bind="physical_address_state" incremental="true()">
               <xf:label>Physical Address State</xf:label>
            </xf:input>
            <xf:input bind="physical_address_country" incremental="true()">
               <xf:label>Physical Address Country</xf:label>
            </xf:input>
            <xf:input bind="physical_address_postcode" incremental="true()">
               <xf:label>Physical Address Postcode</xf:label>
            </xf:input>
            <xf:input bind="postal_address_1" incremental="true()">
               <xf:label>Postal Address 1</xf:label>
            </xf:input>
            <xf:input bind="postal_address_2" incremental="true()">
               <xf:label>Postal Address 2</xf:label>
            </xf:input>
            <xf:input bind="postal_address_state" incremental="true()">
               <xf:label>Postal Address State</xf:label>
            </xf:input>
            <xf:input bind="postal_address_country" incremental="true()">
               <xf:label>Postal Address Country</xf:label>
            </xf:input>
            <xf:input bind="postal_address_postcode" incremental="true()">
               <xf:label>Postal Address Postcode</xf:label>
            </xf:input>
            <xf:submit submission="s01">
               <xf:label>Save</xf:label>
            </xf:submit>
            <xf:trigger>
               <xf:label>Reset</xf:label>
               <xf:reset ev:event="DOMActivate"/>
               <xf:dispatch ev:event="DOMActivate" name="set-selected" target="model1"/>
               <xf:delete ev:event="DOMActivate" nodeset="instance('inst_response')//ServiceException"/>
               <xf:delete ev:event="DOMActivate" nodeset="instance('inst_response')//ogc:FeatureId/@fid"/>
            </xf:trigger>
            <xf:group>
               <xf:output bind="error_message" class="error">
                  <xf:label>Error:</xf:label>
               </xf:output>
               <xf:output bind="success_message">
                  <xf:label>New Record Id:</xf:label>
               </xf:output>
            </xf:group>
         </div>
      </div>
      <br/>
      <div id="console"/>
   </body>
</html>