<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="xsltforms/xsltforms.xsl" type="text/xsl"?><html xmlns:aatams="http://www.imos.org.au/aatams" xmlns="http://www.w3.org/1999/xhtml"
      xmlns:xf="http://www.w3.org/2002/xforms"
      xmlns:ev="http://www.w3.org/2001/xml-events">
   <head>
      <title>AATAMS Web Interface</title>
      <link href="aatams.css" rel="stylesheet" type="text/css"/>
      <xf:model>
         <xf:instance id="inst_request">
            <wfs:GetFeature xmlns:wfs="http://www.opengis.net/wfs" xmlns:gml="http://www.opengis.net/gml"
                            xmlns:ogc="http://www.opengis.net/ogc"
                            outputFormat="text/xml; subtype=gml/3.1.1">
               <wfs:Query typeName="pre3:organisation">
                  <ogc:Filter>
                     <ogc:GmlObjectId gml:id=""/>
                  </ogc:Filter>
               </wfs:Query>
               <aatams:dummy/>
            </wfs:GetFeature>
         </xf:instance>
         <xf:instance id="inst_feature_id">
            <id xmlns="">aatams.organisation.NNNN</id>
         </xf:instance>
         <xf:bind id="feature_id" nodeset="instance('inst_feature_id')" type="xsd:string"/>
         <xf:instance id="inst_response">
            <wfs:FeatureCollection xmlns:wfs="http://www.opengis.net/wfs" xmlns:gml="http://www.opengis.net/gml">
               <gml:featureMember>
                  <aatams:organisation/>
               </gml:featureMember>
            </wfs:FeatureCollection>
         </xf:instance>
         <xf:bind id="name" nodeset="instance('inst_response')//aatams:organisation/aatams:name"
                  type="xsd:string"/>
         <xf:bind id="physical_address_1"
                  nodeset="instance('inst_response')//aatams:organisation/aatams:physical_address_1"
                  type="xsd:string"/>
         <xf:bind id="physical_address_2"
                  nodeset="instance('inst_response')//aatams:organisation/aatams:physical_address_2"
                  type="xsd:string"/>
         <xf:bind id="physical_address_state"
                  nodeset="instance('inst_response')//aatams:organisation/aatams:physical_address_state"
                  type="xsd:string"/>
         <xf:bind id="physical_address_country"
                  nodeset="instance('inst_response')//aatams:organisation/aatams:physical_address_country"
                  type="xsd:string"/>
         <xf:bind id="physical_address_postcode"
                  nodeset="instance('inst_response')//aatams:organisation/aatams:physical_address_postcode"
                  type="xsd:string"/>
         <xf:bind id="postal_address_1"
                  nodeset="instance('inst_response')//aatams:organisation/aatams:postal_address_1"
                  type="xsd:string"/>
         <xf:bind id="postal_address_2"
                  nodeset="instance('inst_response')//aatams:organisation/aatams:postal_address_2"
                  type="xsd:string"/>
         <xf:bind id="postal_address_state"
                  nodeset="instance('inst_response')//aatams:organisation/aatams:postal_address_state"
                  type="xsd:string"/>
         <xf:bind id="postal_address_country"
                  nodeset="instance('inst_response')//aatams:organisation/aatams:postal_address_country"
                  type="xsd:string"/>
         <xf:bind id="postal_address_postcode"
                  nodeset="instance('inst_response')//aatams:organisation/aatams:postal_address_postcode"
                  type="xsd:string"/>
         <xf:bind id="phone_number"
                  nodeset="instance('inst_response')//aatams:organisation/aatams:phone_number"
                  type="xsd:string"/>
         <xf:bind id="fax_number"
                  nodeset="instance('inst_response')//aatams:organisation/aatams:fax_number"
                  type="xsd:string"/>
         <xf:submission id="s01" method="post" action="../../degree-wfs/services"
                        ref="instance('inst_request')"
                        replace="instance"
                        instance="inst_response">
            <xf:message level="modeless" ev:event="xforms-submit-error">Submit error.</xf:message>
         </xf:submission>
      </xf:model>
   </head>
   <body>
      <fieldset xmlns="">
         <legend>VIEW ORGANISATION</legend>
         <table xmlns="http://www.w3.org/1999/xhtml">
            <tbody>
               <tr>
                  <td>
                     <xf:input bind="feature_id">
                        <xf:label>Enter Organisation Id</xf:label>
                     </xf:input>
                  </td>
                  <td>
                     <xf:trigger>
                        <xf:label>Search</xf:label>
                        <xf:action ev:event="DOMActivate">
                           <xf:setvalue ref="instance('inst_request')//@gml:id" value="instance('inst_feature_id')"/>
                           <xf:send submission="s01"/>
                        </xf:action>
                     </xf:trigger>
                  </td>
               </tr>
            </tbody>
         </table>
         <xf:output bind="name">
            <xf:label>Name:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="physical_address_1">
            <xf:label>Physical Address 1:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="physical_address_2">
            <xf:label>Physical Address 2:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="physical_address_state">
            <xf:label>Physical Address State:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="physical_address_country">
            <xf:label>Physical Address Country:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="physical_address_postcode">
            <xf:label>Physical Address Postcode:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="postal_address_1">
            <xf:label>Postal Address 1:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="postal_address_2">
            <xf:label>Postal Address 2:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="postal_address_state">
            <xf:label>Postal Address State:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="postal_address_country">
            <xf:label>Postal Address Country:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="postal_address_postcode">
            <xf:label>Postal Address Postcode:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="phone_number">
            <xf:label>Phone Number:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="fax_number">
            <xf:label>Fax Number:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
      </fieldset>
      <br/>
      <div id="console" style="display:none;"/>
   </body>
</html>