<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet href="xsltforms-beta/xsltforms/xsltforms.xsl" type="text/xsl"?><html xmlns:aatams="http://www.imos.org.au/aatams" xmlns="http://www.w3.org/1999/xhtml"
      xmlns:xf="http://www.w3.org/2002/xforms"
      xmlns:ev="http://www.w3.org/2001/xml-events">
   <head>
      <title>AATAMS Web Interface</title>
      <link href="aatams.css" rel="stylesheet" type="text/css"/>
      <xf:model>
         <xf:instance id="inst_request">
            <wfs:GetFeature xmlns:wfs="http://www.opengis.net/wfs" xmlns:gml="http://www.opengis.net/gml"
                            xmlns:ogc="http://www.opengis.net/ogc"
                            outputFormat="text/xml; subtype=gml/3.1.1">
               <wfs:Query typeName="pre3:deployment_download">
                  <ogc:Filter>
                     <ogc:GmlObjectId gml:id=""/>
                  </ogc:Filter>
               </wfs:Query>
               <aatams:dummy/>
            </wfs:GetFeature>
         </xf:instance>
         <xf:instance id="inst_feature_id">
            <id xmlns="">aatams.deployment_download.NNNN</id>
         </xf:instance>
         <xf:bind id="feature_id" nodeset="instance('inst_feature_id')" type="xsd:string"/>
         <xf:instance id="inst_response">
            <wfs:FeatureCollection xmlns:wfs="http://www.opengis.net/wfs" xmlns:gml="http://www.opengis.net/gml">
               <gml:featureMember>
                  <aatams:deployment_download/>
               </gml:featureMember>
            </wfs:FeatureCollection>
         </xf:instance>
         <xf:bind id="receiver_deployment"
                  nodeset="instance('inst_response')//aatams:receiver_deployment/aatams:name"
                  type="xsd:string"/>
         <xf:bind id="recovery_timestamp"
                  nodeset="instance('inst_response')//aatams:deployment_download/aatams:recovery_timestamp"
                  type="xsd:string"/>
         <xf:bind id="download_timestamp"
                  nodeset="instance('inst_response')//aatams:deployment_download/aatams:download_timestamp"
                  type="xsd:string"/>
         <xf:bind id="time_correction_start"
                  nodeset="instance('inst_response')//aatams:deployment_download/aatams:time_correction_start"
                  type="xsd:string"/>
         <xf:bind id="time_correction_end"
                  nodeset="instance('inst_response')//aatams:deployment_download/aatams:time_correction_end"
                  type="xsd:string"/>
         <xf:bind id="time_correction_type"
                  nodeset="instance('inst_response')//aatams:deployment_download/aatams:time_correction_type"
                  type="xsd:string"/>
         <xf:bind id="clock_drift"
                  nodeset="instance('inst_response')//aatams:deployment_download/aatams:clock_drift"
                  type="xsd:string"/>
         <xf:bind id="pings"
                  nodeset="instance('inst_response')//aatams:deployment_download/aatams:pings"
                  type="xsd:string"/>
         <xf:bind id="detections"
                  nodeset="instance('inst_response')//aatams:deployment_download/aatams:detections"
                  type="xsd:string"/>
         <xf:bind id="filename"
                  nodeset="instance('inst_response')//aatams:deployment_download/aatams:filename"
                  type="xsd:string"/>
         <xf:bind id="comments"
                  nodeset="instance('inst_response')//aatams:deployment_download/aatams:comments"
                  type="xsd:string"/>
         <xf:submission id="s01" method="post" action="../deegree-wfs/services"
                        ref="instance('inst_request')"
                        replace="instance"
                        instance="inst_response">
            <xf:message level="modeless" ev:event="xforms-submit-error">Submit error.</xf:message>
         </xf:submission>
      </xf:model>
   </head>
   <body>
      <fieldset xmlns="">
         <legend>VIEW DEPLOYMENT DOWNLOAD</legend>
         <table xmlns="http://www.w3.org/1999/xhtml">
            <tbody>
               <tr>
                  <td>
                     <xf:input bind="feature_id">
                        <xf:label>Enter Deployment Download Id</xf:label>
                     </xf:input>
                  </td>
                  <td>
                     <xf:trigger>
                        <xf:label>Search</xf:label>
                        <xf:action ev:event="DOMActivate">
                           <xf:setvalue ref="instance('inst_request')//@gml:id" value="instance('inst_feature_id')"/>
                           <xf:send submission="s01"/>
                        </xf:action>
                     </xf:trigger>
                  </td>
               </tr>
            </tbody>
         </table>
         <xf:output bind="receiver_deployment">
            <xf:label>Receiver Deployment:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="recovery_timestamp">
            <xf:label>Recovery Timestamp:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="download_timestamp">
            <xf:label>Download Timestamp:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="time_correction_start">
            <xf:label>Time Correction Start:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="time_correction_end">
            <xf:label>Time Correction End:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="time_correction_type">
            <xf:label>Time Correction Type:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="clock_drift">
            <xf:label>Clock Drift:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="pings">
            <xf:label>Pings:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="detections">
            <xf:label>Detections:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="filename">
            <xf:label>Filename:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
         <xf:output bind="comments">
            <xf:label>Comments:</xf:label>
            <xf:help>This is a presently unhelpful help message</xf:help>
         </xf:output>
      </fieldset>
      <br/>
      <div id="console" style="display:none;"/>
   </body>
</html>