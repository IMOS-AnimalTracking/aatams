					<xf:help>This is a presently unhelpful help message</xf:help>
				</xf:output>
				<xf:output bind="deployment_timestamp">
					<xf:label>Deployment Timestamp:</xf:label>
					<xf:help>This is a presently unhelpful help message</xf:help>
				</xf:output>
				<xf:output bind="acoustic_release">
					<xf:label>Acoustic Release:</xf:label>
					<xf:help>This is a presently unhelpful help message</xf:help>
				</xf:output>
				<xf:output bind="mooring_type">
					<xf:label>Mooring Type:</xf:label>
					<xf:help>This is a presently unhelpful help message</xf:help>
				</xf:output>
				<xf:output bind="bottom_depth">
					<xf:label>Bottom Depth:</xf:label>
					<xf:help>This is a presently unhelpful help message</xf:help>
				</xf:output>
				<xf:output bind="comments">
					<xf:label>Comments:</xf:label>
					<xf:help>This is a presently unhelpful help message</xf:help>
				</xf:output>
			</xf:case>
		</xf:switch>
